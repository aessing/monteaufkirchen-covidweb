{
  "routes": [
    {
      "route": "/admin/*",
      "allowedRoles": ["administrator"]
    },
    {
      "route": "/api/GetTestStatus",
      "methods": ["POST"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/*",
      "methods": ["POST"],
      "allowedRoles": ["administrator"]
    },
    {
      "route": "/login",
      "rewrite": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    }
  ]
}
